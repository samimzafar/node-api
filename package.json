{
  "name": "node-api",
  "version": "1.0.0",
  "description": "Hype API's",
  "scripts": {
    "start": "node server.js | node_modules/.bin/bunyan",
    "server": "nodemon server.js | node_modules/.bin/bunyan",
    "lint": "npx eslint .",
    "prettier": "npx prettier -c .",
    "prettify": "npx prettier -w .",
    "db:migrate": "npx sequelize db:migrate",
    "db:migrate:create": "npx sequelize migration:generate --name",
    "db:migrate:undo": "npx sequelize db:migrate:undo",
    "db:migrate:status": "npx sequelize db:migrate:status"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "axios": "^1.3.5",
    "bcrypt": "^5.1.0",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-bunyan-logger": "^1.3.3",
    "firebase-admin": "^11.5.0",
    "joi": "^17.8.4",
    "jsonwebtoken": "^9.0.0",
    "moment-timezone": "^0.5.43",
    "mysql2": "^2.3.3",
    "pm2": "^5.2.2",
    "sequelize": "^6.28.0"
  },
  "devDependencies": {
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.3",
    "nyc": "^15.1.0",
    "prettier": "2.8.2",
    "pretty-quick": "^3.1.3",
    "sequelize-cli": "^6.5.2"
  },
  "engines": {
    "node": "18.0.0",
    "npm": "9.6.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
